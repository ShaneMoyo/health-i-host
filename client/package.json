{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-scripts": "1.1.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "superagent": "^3.8.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build && npm run copy",
    "copy": "rimraf ../server/public && copyfiles -R ./build/* ../server/public",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "build-css": "node-sass-chokidar --include-path ./src/styles src/ -o src/"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "bulma": "^0.6.2",
    "node-sass-chokidar": "0.0.3"
  },
  "proxy": {
    "/api": {
      "target": "http://localhost:3001"
    }
  }
}
